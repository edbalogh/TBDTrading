{
  "name": "tbd-trading",
  "bin": {
    "tbd": "tbd.ts"
  },
  "version": "0.0.1",
  "description": "TBDTrading initialised with create-express-typescript-application.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "node ./build/src/index.js",
    "dev": "ts-node ./src/index.ts",
    "dev:nodemon": "nodemon -w src -e ts,json -x ts-node ./src/index.ts",
    "test": "jest --coverage",
    "start-broker-server": "tbd wsServer -t Broker -i binance",
    "start-market-server": "tbd wsServer -t MarketData -i binance",
    "start-basic-grid": "tbd startExampleBot -b basic-grid-bot"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.45",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "nodemon": "^2.0.7",
    "socket.io-await-test": "^1.0.3",
    "technicalindicators": "^3.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.169",
    "@types/mongodb": "^3.6.18",
    "@types/shortid": "^0.0.29",
    "@types/socket.io": "^3.0.2",
    "@types/ws": "^7.4.4",
    "@types/yargs": "^15.0.13",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "mongodb": "^3.6.9",
    "morgan": "^1.10.0",
    "reconnecting-websocket": "^4.4.0",
    "shortid": "^2.2.16",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "ts-jest": "^26.5.6",
    "ws": "^7.4.6",
    "yargs": "^15.4.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  }
}
